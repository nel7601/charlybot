# Modbus Configuration
# Copy this file to .env and update with your robot's IP address

# Robot Modbus TCP Host (IP address of your robot)
MODBUS_HOST=192.168.1.100

# Robot Modbus TCP Port (usually 502)
MODBUS_PORT=502

# Robot Modbus Unit ID (usually 1)
MODBUS_UNIT_ID=1

# Modbus Timeout in milliseconds
MODBUS_TIMEOUT=3000
